{
  "name": "@lamnhan/ngx-useful",
  "version": "0.0.0",
  "description": "A collection of helpful Angular services, pipes, ....",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:lib": "ng build useful --prod && cpy README.md LICENSE dist/useful",
    "build:app": "ng build --prod && ayedocs generate",
    "build": "npm run build:app && npm run build:lib",
    "deploy:lib": "cd ./dist/useful && npm publish && cd ../../",
    "deploy:app": "git push",
    "deploy": "npm run deploy:app && npm deploy:lib",
    "lint": "ng lint useful",
    "test": "ng test useful",
    "e2e": "ng e2e",
    "coverage": "cat ./coverage/*/lcov.info | coveralls",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "keywords": [
    "angular",
    "service",
    "pipe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/ngx-useful.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/ngx-useful/issues"
  },
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@lamnhan/schemata": "0.0.6",
    "@ngneat/transloco": "^2.20.1",
    "firebase": "^8.2.9",
    "howler": "^2.2.1",
    "localforage": "^1.9.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/howler": "^2.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
